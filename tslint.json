{
    "rules": {
        "class-name": true,
        "comment-format": false,
        "indent": false,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-internal-module": false,
        "no-trailing-whitespace": true,
        "no-var-keyword": true,
        "one-line": false,
        "quotemark": false,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "typedef-whitespace": true,
        "variable-name": false,
        "whitespace": false,
        "no-conditional-assignment": true
    }
}
